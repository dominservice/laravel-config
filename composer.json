{
  "name": "dominservice/laravel-stripe",
  "description": "Laravel integration for Stripe.",
  "type": "library",
  "keywords": ["laravel", "stripe"],
  "homepage": "https://github.com/dominservice/laravel-stripe",
  "license": "MIT",
  "authors": [
    {
      "name": "DSO-IT Mateusz Domin",
      "email": "biuro@dso.biz.pl"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "illuminate/console": "^10|^11",
    "illuminate/contracts": "^10|^11",
    "illuminate/database": "^10|^11",
    "illuminate/routing": "^10|^11",
    "illuminate/support": "^10|^11"
  },
  "autoload": {
    "psr-4": {
      "Dominservice\\Config\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dominservice\\Config\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
